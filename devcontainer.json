{
  "name": "Java/Kotlin Dev Container",
  "image": "mcr.microsoft.com/devcontainers/java:0-17",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17"
    }
  },
  "extensions": [
    "vscjava.vscode-java-pack",
    "fwcd.kotlin",
    "redhat.java"
  ],
  "postCreateCommand": "java -version && kotlinc -version",
  "forwardPorts": [],
  "remoteUser": "vscode"
}
